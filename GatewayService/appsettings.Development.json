{
  "ReverseProxy": {
    "Routes": {
      "CarService": {
        "ClusterId": "CarService",
        "Match": {
          "Path": "car/api/{version}/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/{version}/{**catchall}"
          }
        ]
      },
      "HotelService": {
        "ClusterId": "HotelService",
        "Match": {
          "Path": "hotel/api/{version}/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/{version}/{**catchall}"
          }
        ]
      },
      "FlightService": {
        "ClusterId": "FlightService",
        "Match": {
          "Path": "flight/api/{version}/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/{version}/{**catchall}"
          }
        ]
      },
      "VacationService": {
        "ClusterId": "VacationService",
        "Match": {
          "Path": "vacation/api/{version}/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/{version}/{**catchall}"
          }
        ]
      }
    },
    "Clusters": {
      "CarService": {
        "Destinations": {
          "Destination": {
            "Address": "http://localhost:5003"
          }
        }
      },
      "HotelService": {
        "Destinations": {
          "Destination": {
            "Address": "http://localhost:5002"
          }
        }
      },
      "FlightService": {
        "Destinations": {
          "Destination": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "VacationService": {
        "Destinations": {
          "Destination": {
            "Address": "http://localhost:5000"
          }
        }
      }
    }
  }
}